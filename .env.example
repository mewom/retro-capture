# Cloudflare R2 Configuration
# Get your Account ID from Cloudflare dashboard URL: dash.cloudflare.com/[THIS_IS_YOUR_ACCOUNT_ID]

# Your Cloudflare Account ID
CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# Your Cloudflare API Token (the one you created with R2 permissions)
CLOUDFLARE_API_TOKEN=your_api_token_here

# R2 Bucket Name (the bucket you created)
R2_BUCKET_NAME=retro-capture-videos

# Server Port (leave as 3000 unless you have a reason to change it)
PORT=3000
