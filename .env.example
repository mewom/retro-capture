# Cloudflare R2 Configuration (S3-compatible)

# R2 Access Key ID (from R2 API Tokens page)
R2_ACCESS_KEY_ID=your_r2_access_key_id

# R2 Secret Access Key (from R2 API Tokens page)
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key

# R2 Endpoint URL (from R2 API Tokens page)
R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com

# R2 Bucket Name
R2_BUCKET_NAME=retro-capture-videos

# Server Port
PORT=3000
